fun temp(a: str, b: str, c: str) {
    print(a)
    x := "Hello"
    print(x)
    y := "World"
    print(c)
    print(y)
    print(b)
}
temp("AAAA", "BBBB", "CCCC")

fun bla(x: str) {
    print(x)
}
bla("Hello")
temp("aaaa", "bbbb", "cccc")


